    <uc1:header runat="server" id="header" />

    <main role="main" id="main">

      <uc1:content runat="server" id="content" />
      <asp:contentplaceholder id="cpcontent" runat="server" />

    </main>

    <uc1:footer runat="server" id="footer" />

  </form>

  <!-- Vendor JS -->
